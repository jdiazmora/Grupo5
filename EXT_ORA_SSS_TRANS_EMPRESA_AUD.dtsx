      DTS:EvaluateAsExpression="True"
      DTS:Expression="(&quot;SELECT     O.OR_ORDEN_BANCO, D .DT_ORDEN_BANCO , O.OR_ORDEN_EMPRESA, O.OR_ORDENANTE, O.OR_NEM_ORDENANTE, O.OR_PRODUCTO, O.OR_SERVICIO, O.OR_REFERENCIA, &#xA;O.OR_USR_GENERACION, O.OR_FCH_GENERACIONH, O.OR_FCH_TRANSMISIONH, O.OR_FCH_INICIO_PAGCOB, O.OR_FCH_VCTO, O.OR_FCH_REPROCESO, O.OR_PAIS_CTA, O.OR_TIPO_CTA, &#xA;O.OR_NUMERO_CTA, O.OR_BD_ACTIVO, O.OR_LOCALIDAD_ORIGEN, O.OR_DIR_IP, O.OR_USR_TRANSMISION, O.OR_NOM_ARCHIVO_TRANSMISION, O.OR_LOTE_ARCHIVO_TRANSMISION, &#xA;O.OR_CANT_ORDENES_SOLICITADAS, O.OR_VALOR_TOTAL_ORDENADO, O.OR_VALOR_TOTAL_APROBADO, O.OR_EST_APROBACION, O.OR_EST_TRANSMISION, D .DT_SECUENCIAL, D .DT_NUC_ORDENANTE, &#xA;D .DT_ORDENANTE, D .DT_REFERENCIA_GRUPO, D .DT_COD_TERCERO_EMPRESA, D .DT_NEM_ORDENANTE, D .DT_CONCEPTO, D .DT_MONEDA_ORDEN, D .DT_VALOR_ORDENADO, &#xA;D .DT_MONEDA_APROBACION, D .DT_VALOR_APROBADO, D .DT_SALDO, D .DT_COD_BANCO_CTA, D .DT_TIPO_CTA, D .DT_NUMERO_CUENTA, D .DT_PRODUCTO, D .DT_SERVICIO, &#xA;D .DT_TIPO_NUC_BENEFICIARIO, D .DT_NUC_BENEFICIARIO, D .DT_NOMBRE_BENEFICIARIO, D .DT_OFICINA_PAGCOB, D .DT_LOCALIDAD_PAGCOB, D .DT_FCH_VENCIMIENTO, D .DT_BD_SWIFT, &#xA;D .DT_FCH_PAGCOB, D .DT_FCH_ULTIMO_PROCESO, D .DT_EST_DETALLE, D .DT_COD_ERROR, D .DT_EST_TRANSMISION, A.AU_COD_EMPRESA, A.AU_COD_PRODUCTO, A.AU_COD_SERVICIO, &#xA;A.AU_SP_EJECUCION, A.AU_DESCRIPCION_ERROR, A.AU_FECHOR_REGISTRO, A.AU_COD_MODULO, A.AU_COD_TRANSACCION, A.AU_ESTACION, P.AP_COD_PRODUCTO, P.AP_NOM_PRODUCTO, &#xA;S.SE_COD_SERVICIO, S.SE_NOM_SERVICIO, L.LO_FCH_INGRESO, L.LO_FCH_SALIDA, L.LO_APLICACION, RU.RU_COD_USUARIO, RU.RU_USUARIO_INGRESO, E.EM_NOM_EMPRESA, U.US_ID_USUARIO, &#xA;ORD.OR_NUMERO_CTA AS NUMERO_CTA_SNT&#xA;FROM            SNT_BZP_CRG_ORDEN O &#xA;INNER JOIN SNT_BZP_CRG_DETALLE D ON O.OR_ORDEN_BANCO = D .DT_ORDEN_BANCO&#xA;LEFT JOIN SNT_BZA_AUDITORIA A ON O.OR_ORDEN_EMPRESA = A.AU_COD_EMPRESA &#xA;LEFT JOIN SNT_BZA_PRODUCTO P ON O.OR_PRODUCTO = P.AP_COD_PRODUCTO &#xA;LEFT JOIN SNT_BZA_SERVICIO S ON O.OR_SERVICIO = S.SE_COD_SERVICIO AND O.OR_PRODUCTO = S.SE_COD_PRODUCTO &#xA;LEFT JOIN SNT_BZA_LOGIN L ON O.OR_ORDEN_EMPRESA = L.LO_COD_EMPRESA &#xA;LEFT JOIN SNT_BZA_USUARIO U ON O.OR_ORDEN_EMPRESA = U.US_COD_EMPRESA &#xA;LEFT JOIN SNT_BZA_ROL_USUARIO RU ON O.OR_ORDEN_EMPRESA = RU.RU_COD_EMPRESA AND RU.RU_COD_USUARIO = U.US_COD_USUARIO &#xA;LEFT JOIN SNT_BZA_EMPRESA E ON O.OR_ORDEN_EMPRESA = E.EM_COD_EMPRESA &#xA;LEFT JOIN SNT_BZP_CRG_ORDEN ORD ON O.OR_ORDEN_EMPRESA = ORD.OR_ORDEN_EMPRESA&#xA;WHERE ROWNUM=1000&#xA;ORDER BY O.OR_ORDEN_BANCO ASC&quot;)"
